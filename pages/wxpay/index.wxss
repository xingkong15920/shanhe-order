.pay_body {
    background: #f7f7f7;
    height: 100vh;
}

.pay-shop-info {
    width: 100%;
    margin-bottom: 2rem;
}

.pay-shop-info .content {
    padding-top: 3rem;
    display: flex;
    justify-content: center;
    width: 80%;
    font-size: 0.32rem;
    margin: 0 auto;
}

.pay-shop-info .shop-logo {
    width: 3rem;
    height: 3rem;
}

.pay-shop-info .shop-name {
    flex: auto;
    font-size: 0.32rem;
    margin-left: 0.2rem;
    color: #2f323a;
    overflow: hidden;
}

.shop-myEqument_name {
    font-size: 1rem;
    text-align: center;
    line-height: 3rem;
    margin-left: 10px;
    font-weight: bold;
}

.pay-shop-info .shop-myEqument {
    display: block;
    width: 100%;
    font-size: 1rem;
    color: #ff6347;
    text-align: center;
}

.iconbg {
    height: 30px;
    width: 30px;
    text-align: center;
    line-height: 30px;
    background: rgb(220, 80, 70);
    border-radius: 50%;
}

.pay-input-money {
    width: 85%;
    overflow: hidden;
    line-height: 3.5rem;
    margin: 0 auto;
    background: #fff;
}

.input-left {
    width: 45%;
    font-size: 1rem;
    padding-left: 10px;
    line-height: 3rem;
    color: #999;
}

.input-right {
    width: 100%;
    font-size: 2.4rem;
    text-align: right;
    padding: 0 10px;
    box-sizing: border-box;
    text-align: left;
}

.input-right label {
    font-size: 2.4rem;
}

.upper-case {
    margin-right: 0.48rem;
    font-size: 0.25rem;
    color: #949494;
    float: right;
}



.payinfo{text-align: center;height: 50%;width: 100%;position: fixed;bottom: 0;background: #fff;}
.keyboard_l{width: 75%;float: left;height: 100%;}
.paynum{width: 33.33%;height:25%;float: left;border-right: 1px solid #D9D9D9;border-bottom: 1px solid #D9D9D9;box-sizing: border-box;font-size: 60rpx;line-height: 150rpx;}
.paynum.colspan{width: 66.66%;}

.keyboard_r{width: 25%;float: right;height: 100%;}
.pay_tui{width: 100%;height: 25%;border-bottom: 1px solid #D9D9D9;box-sizing: border-box;line-height: 150rpx}
.icon-tuigejian:before{font-size: 60rpx;}
.pay_again{width: 100%;height: 25%;box-sizing: border-box;border-bottom: 1px solid #D9D9D9;padding: 25% 25%;}
.pay_zhifu{width: 100%;height: 100%;box-sizing: border-box;padding: 80% 25% 0;background: #d9d9d9;margin-left:-1px;
}
.pay_zhifu.active{background:#fe9b20}







.payinfo .paynum {
    color: #333;
}

.payinfo .paynum1 {
    color: #424857;
    font-size: 1rem;
}

.payinfo .payzero {
    color: #424857;
}

table {
    width: 100%;
    height: 50%;
    position: absolute;
    bottom: 0;
    background-color: white;
    background-top: none;
}

table tr td {
    text-align: center;
    width: 1.88rem;
    height: 1.26rem;
    font-size: 1.8rem;
    font-family: "Microsoft YaHei";
    font-weight: 400;
    border-right: 1px solid #d9d9d9;
    border-bottom: 1px solid #d9d9d9;
}

table tr td:active {
    background-color: #ececec;
}

.keybord-return {
    width: 1.88rem;
    height: 1.26rem;
}

.keybord-stop {
    background-image: url(../images/keybord-stop.png);
    background-size: 90% 90%;
}

.pay {
    color: #fff;
    font-size: 1.2rem;
    background-color: #fe9b20;
}

.pay:active {
    background-color: #d9d9d9;
}

.pay a {
    display: block;
    position: relative;
    width: 100%;
    height: 100%;
    color: #fff;
    text-decoration: none;
}

.a-pay {
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
}

.pay-disabled {
    background-color: #d9d9d9;
}

#pay-return i {
    font-size: 1.5rem;
}

